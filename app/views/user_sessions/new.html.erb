<div class="signup">

<h3>Log in to DotaBets</h3>



<div class="login">
<%= form_tag user_sessions_path do %>
  <div class="field">
    <%= label_tag :email %><br/>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <div class="action">
  	
  	<%= button_tag(type: "submit", class: "btn btn-primary") do %>
    <i class="icon-ok"></i> Login 
    <% end %>
  </div>
<% end %>
</div>
</div>